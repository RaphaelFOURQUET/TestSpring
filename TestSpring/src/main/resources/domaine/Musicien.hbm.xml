<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
  
<hibernate-mapping> 
  
    <class name="fr.adaming.entity.Musicien" table="MUSICIEN">
    	<property name="id" type="int">
    		<column name="id"/>
    	</property>
    	<property name="name" type="string">
    		<column name="name"/>
    	</property>
    	<property name="morceau" type="string">
    		<column name="morceau"/>
    	</property>
    	<property name="instrument" type="fr.adaming.entity.Instrument"> <!-- A verifier -->
    		<column name="instrument"></column>
    	</property>
  
    </class> 
  
</hibernate-mapping>